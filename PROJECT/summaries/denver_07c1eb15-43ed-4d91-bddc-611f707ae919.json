            "keywords": [
                "byob",
                "street",
                "platt",
                "1810",
                "want"
            ]
        },
        {
            "timestamp": "0:01:54",
            "text": "This Thursday, I've announced it on Facebook and I would love anyone and everybody to stop\nby and see my beautiful space, thank you.",
            "keywords": [
                "facebook",
                "announced",
                "thursday",
                "beautiful",
                "love"
            ]
